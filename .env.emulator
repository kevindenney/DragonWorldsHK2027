# Firebase Emulator Configuration
# This file contains the emulator-specific environment variables
# Use this for local development with Firebase emulators

# Firebase Project Configuration (Use demo project for emulators)
EXPO_PUBLIC_FIREBASE_PROJECT_ID=demo-dragonworldshk2027
EXPO_PUBLIC_FIREBASE_API_KEY=demo-api-key
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=demo-dragonworldshk2027.firebaseapp.com
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=demo-dragonworldshk2027.appspot.com
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
EXPO_PUBLIC_FIREBASE_APP_ID=1:123456789:web:demo
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=G-DEMO

# Development Environment Settings
EXPO_PUBLIC_NODE_ENV=development
EXPO_PUBLIC_DEBUG_MODE=true

# Emulator Configuration
EXPO_PUBLIC_EMULATOR_HOST=localhost

# Backend API Configuration (Points to emulator)
EXPO_PUBLIC_BACKEND_URL=http://localhost:5001/demo-dragonworldshk2027/us-central1/api
EXPO_PUBLIC_API_VERSION=v1

# OAuth Configuration (Disabled for emulator)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# Feature Flags (Disabled for emulator)
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASHLYTICS=false
EXPO_PUBLIC_ENABLE_PERFORMANCE_MONITORING=false

# Development Settings
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_MOCK_API_RESPONSES=false
EXPO_PUBLIC_BYPASS_AUTH=false