{
  "name": "dragonworldshk2027",
  "version": "1.0.2",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "expo:go": "expo start",
    "postinstall": "npx --no patch-package 2>/dev/null || true && node scripts/fix-firebase-postinstall.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev:ios": "expo run:ios --device",
    "dev:android": "expo run:android --device",
    "web": "expo start --web",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:accessibility": "jest --testNamePattern=\"accessibility\"",
    "test:firebase": "node --experimental-vm-modules node_modules/.bin/jest src/__tests__/firebase --testPathIgnorePatterns='[]' --testTimeout=30000 --forceExit",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "build:dev": "eas build --profile development",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update": "eas update",
    "bundle-analyze": "npx expo export && npx expo-bundle-analyzer",
    "build:analyze": "node scripts/analyze-bundle.js",
    "optimize": "npm run build:analyze && npm run lint && npm run typecheck",
    "bundle:ios": "./scripts/build.sh -p ios -e production",
    "bundle:android": "./scripts/build.sh -p android -e production",
    "bundle:all": "./scripts/build.sh -p all -e production",
    "prepare-submission": "./scripts/prepare-submission.sh",
    "prepare-submission:ios": "./scripts/prepare-submission.sh -p ios",
    "prepare-submission:android": "./scripts/prepare-submission.sh -p android",
    "validate-build": "./scripts/post-build-validation.sh",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test:monetization": "ts-node scripts/testMonetizationFeatures.ts",
    "test:monetization:quick": "ts-node scripts/testMonetizationFeatures.ts --quick",
    "deploy:check": "ts-node scripts/deploymentReadinessCheck.ts",
    "deploy:validate": "npm run deploy:check && npm run test:monetization && npm run typecheck && npm run lint",
    "emulator:start": "firebase emulators:start",
    "emulator:start:import": "firebase emulators:start --import=./emulator-data",
    "emulator:export": "firebase emulators:export ./emulator-data",
    "emulator:kill": "npx kill-port 4000 5000 5001 8080 9099 9199",
    "emulator:seed": "node scripts/seedEmulatorData.js",
    "dev": "npm run emulator:start:import",
    "dev:fresh": "npm run emulator:kill && npm run emulator:start",
    "dev:setup": "bash scripts/dev-setup.sh",
    "emulator:health": "node scripts/emulator-health-check.js",
    "test:firebase": "jest src/__tests__/firebase --testTimeout=30000",
    "test:rules": "jest src/__tests__/firebase/firestore.rules.test.ts --testTimeout=30000",
    "test:integration": "jest src/__tests__/firebase/emulator.integration.test.ts --testTimeout=30000",
    "dev:api": "vercel dev",
    "deploy:api": "vercel --prod",
    "test:api": "curl \"http://localhost:3000/api/scrape-race-data?eventId=dragon-worlds-2027&type=event\"",
    "maestro:test": "maestro test e2e/maestro/flows/",
    "maestro:test:onboarding": "maestro test e2e/maestro/flows/onboarding.yaml",
    "maestro:test:navigation": "maestro test e2e/maestro/flows/navigation.yaml",
    "maestro:test:schedule": "maestro test e2e/maestro/flows/schedule.yaml",
    "maestro:test:notices": "maestro test e2e/maestro/flows/notices.yaml",
    "maestro:test:results": "maestro test e2e/maestro/flows/results.yaml",
    "maestro:test:more": "maestro test e2e/maestro/flows/more.yaml",
    "maestro:record": "maestro record",
    "maestro:studio": "maestro studio",
    "build:simulator": "eas build --profile simulator --platform ios",
    "build:simulator:android": "eas build --profile simulator --platform android"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/metro-runtime": "~5.0.5",
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-google-signin/google-signin": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.95.2",
    "@tanstack/react-query": "^5.90.20",
    "event-target-shim": "^6.0.2",
    "expo": "~53.0.26",
    "expo-apple-authentication": "~7.2.4",
    "expo-asset": "^11.1.7",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-crypto": "~14.1.5",
    "expo-device": "^7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image-manipulator": "~13.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-location": "~18.1.6",
    "expo-notifications": "~0.31.5",
    "expo-sharing": "~13.1.5",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.18",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.0.0",
    "jest-expo": "~53.0.14",
    "lucide-react-native": "^0.542.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-maps": "1.20.1",
    "react-native-qrcode-svg": "^6.3.15",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "typescript": "~5.8.3",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/cli": "^0.24.24",
    "@firebase/rules-unit-testing": "^5.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "expo-dev-client": "~5.2.4",
    "firebase-tools": "^14.15.2",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.14",
    "kill-port": "^2.0.1",
    "msw": "^2.1.5",
    "patch-package": "^8.0.1",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect",
      "<rootDir>/src/testing/setupTests.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/testing/**/*",
      "!src/**/__tests__/**/*",
      "!src/**/*.test.*",
      "!src/**/*.spec.*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "src/__tests__/firebase/"
    ]
  }
}
